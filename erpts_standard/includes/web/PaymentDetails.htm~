function MM_preloadImages() { //v3.0
  var d=document; if(d.images){ if(!d.MM_p) d.MM_p=new Array();
    var i,j=d.MM_p.length,a=MM_preloadImages.arguments; for(i=0; i<a.length; i++)
    if (a[i].indexOf("#")!=0){ d.MM_p[j]=new Image; d.MM_p[j++].src=a[i];}}
}

function MM_findObj(n, d) { //v4.01
  var p,i,x;  if(!d) d=document; if((p=n.indexOf("?"))>0&&parent.frames.length) {
    d=parent.frames[n.substring(p+1)].document; n=n.substring(0,p);}
  if(!(x=d[n])&&d.all) x=d.all[n]; for (i=0;!x&&i<d.forms.length;i++) x=d.forms[i][n];
  for(i=0;!x&&d.layers&&i<d.layers.length;i++) x=MM_findObj(n,d.layers[i].document);
  if(!x && d.getElementById) x=d.getElementById(n); return x;
}
function openWin(strLink, strWindow, intLeft, intTop, intWidth, intHeight){
	puffup = window.open(strLink, strWindow, "position:absolute; left="+intLeft+", top="+intTop+", width="+intWidth+", height="+intHeight+", toolbar=no, scrollbars=yes, resizable=yes, menubar=no, location=no, status=no");
	puffup.focus(true);
}
function checkAction(axn){
	document.OwnerList.formAction.value = axn;
	if (confirmSubmit())
	document.OwnerList.submit();
}
function confirmSubmit(){
	axn = document.OwnerList.formAction.value;
	if(!confirm(axn+' selected items?')){
		return false;
	}
	else
		return true;
}

function btToggle(id){
 	obj=document.getElementById('btTdRow'+id);
 	if (obj.style.display=='none') {
		obj.style.display='';
	}
	else {
		obj.style.display='none';
	}
}


function toggle(id){
 	obj=document.getElementById('tdRow'+id);
 	if (obj.style.display=='none') {
		obj.style.display='';
		location.href = '#td'+id;
	}
	else {
		obj.style.display='none';
		location.href = '#td';
	}
}
function toggleShowHide(){
	obj = eval('document.RPTOPForm.tdShowHide');
	objArray = 'document.RPTOPForm.tdChk';
	tdCtr = {tdCtr};
	count = tdCtr;
	if (obj.checked){
		toggleAll('');
		i = 0;
		while (i < count){
			eval(objArray+i+'.checked = true');
			i++;
		}
	}
	else {
		toggleAll('none');
		i = 0;
		while (i < count){
			//alert('hello');
			eval(objArray+i+'.checked = false');
			i++;
		}
	}
}
function toggleAll(action){
	<!-- BEGIN toggleTDList -->
		document.getElementById('tdRow{ctr}').style.display=action;
	<!-- END toggleTDList -->
}

function toggleBacktaxTD(id){
 	obj=document.getElementById('backtaxTD'+id);
 	if (obj.style.display=='none') {
		obj.style.display='';
	}
	else {
		obj.style.display='none';
	}
}

function changeDueTypeValues(id){
	h = 0;
	v = 0;

	dueType = "Annual"; //default
	
	dueTypeField = getFormElementByName("td["+id+"][dueType]");
	dueType = dueTypeField.options[dueTypeField.selectedIndex].value;

	for(i=0 ; i<document.RPTOPForm.elements.length ; i++){
		elementName = document.RPTOPForm.elements[i].name;

		if(elementName.indexOf("bt") <= 0){
			if(document.RPTOPForm.elements[i].type=="hidden"){
				if(elementName.indexOf("["+dueType+"]") >= 0){
					if(elementName.indexOf("]"+id) >= 0){
						stripBrackets = elementName.substring(0,elementName.indexOf("["));
						visibleField = getFormElementByName("td["+id+"]["+stripBrackets+"]");
						visibleField.value = document.RPTOPForm.elements[i].value;
					}
				}
			}
		}
	}
	computeBalance(id,true);
}

function getFormElementByName(elementName){
	if(document.getElementById(elementName)){
		return document.getElementById(elementName);
	}
	else{
		return false;
	}
}

var taxDueArray = new Array();
var	balanceDueArray = new Array();

function initUpdateTotalArrays(){
	j=0;
	for(i=0 ; i<document.RPTOPForm.elements.length ; i++){
		elementName = document.RPTOPForm.elements[i].name;

		if(elementName.indexOf("[balance2]") >= 0){
			balanceDueArray[j] = document.RPTOPForm.elements[i];
		}

		if(elementName.indexOf("[taxDue]") >= 0){
			taxDueArray[j] = document.RPTOPForm.elements[i];
			j++;
		}
	}
}

function updateTotals(){
	totalTaxDueField = document.totalsForm.totalTaxDue;
	totalNetDueField = document.totalsForm.totalNetDue;

	totalTaxDueValue = 0;
	totalNetDueValue = 0;

	for(k=0; k < taxDueArray.length ; k++){
		totalTaxDueValue += parseFloat(unformatCurrency(taxDueArray[k].value));
		totalNetDueValue += parseFloat(unformatCurrency(balanceDueArray[k].value));
	}

	totalTaxDueField.value = formatCurrency(totalTaxDueValue);
	totalNetDueField.value = formatCurrency(totalNetDueValue);
}

function computeBalance(id,doUpdateTotal){
	balanceField = getFormElementByName("td["+id+"][balance]");
	balanceField2 = getFormElementByName("td["+id+"][balance2]");

	taxDueField = getFormElementByName("td["+id+"][taxDue]");
//	taxDueField2 = getFormElementByName("td["+id+"][taxDue2]");

	earlyPaymentDiscountField = getFormElementByName("td["+id+"][earlyPaymentDiscount]");
	advancedPaymentDiscountField = getFormElementByName("td["+id+"][advancedPaymentDiscount]");
	amenstyField = getFormElementByName("td["+id+"][amnesty]");
	penaltyField = getFormElementByName("td["+id+"][penalty]");
	totalPaidField = getFormElementByName("td["+id+"][totalPaid]");

	balance = parseFloat(unformatCurrency(balanceField.value));
	taxDue = parseFloat(unformatCurrency(taxDueField.value));
	earlyPaymentDiscount = parseFloat(unformatCurrency(earlyPaymentDiscountField.value));
	advancedPaymentDiscount = parseFloat(unformatCurrency(advancedPaymentDiscountField.value));
	penalty = parseFloat(unformatCurrency(penaltyField.value));
	totalPaid = parseFloat(unformatCurrency(totalPaidField.value));

	balance = taxDue;
	if(advancedPaymentDiscount > 0){
		balance = taxDue - advancedPaymentDiscount;
	}
	else if(earlyPaymentDiscount > 0){
		balance = taxDue - earlyPaymentDiscount;
	}

	if(!amenstyField.checked){
		balance = balance + penalty;
		balance = balance - totalPaid;
	}
	else{
		balance = balance - totalPaid;
		if(balance<0){
			balance = 0;
		}		
	}


	balanceField.value = formatCurrency(balance.toString());
	balanceField2.value = balanceField.value;

	if(doUpdateTotal==true){
		updateTotals();
	}
//	taxDueField2.value = taxDueField.value;
}

function btChangeDueTypeValues(tdID,btTDID){
	h = 0;
	v = 0;
	stripBrackets = "";

	dueType = "Annual"; //default
	
	dueTypeField = getFormElementByName("td["+tdID+"][btTD]["+btTDID+"][dueType]");
	dueType = dueTypeField.options[dueTypeField.selectedIndex].value;

	for(i=0 ; i<document.RPTOPForm.elements.length ; i++){
		elementName = document.RPTOPForm.elements[i].name;

		if(elementName.indexOf("bt") >= 0){
			if(document.RPTOPForm.elements[i].type=="hidden"){
				if(elementName.indexOf("["+dueType+"]") >= 0){
					if(elementName.indexOf("]"+btTDID) >= 0){
						stripBrackets = elementName.substring(2,elementName.indexOf("["));

						// to "un-propercase"
						firstChar = stripBrackets.charAt(0);
						firstChar = firstChar.toLowerCase();
						stripBrackets = firstChar+stripBrackets.substring(1,stripBrackets.length);
	
						visibleField = getFormElementByName("td["+tdID+"][btTD]["+btTDID+"]["+stripBrackets+"]");
						visibleField.value = document.RPTOPForm.elements[i].value;
					}
				}
			}
		}
	}
	btComputeBalance(tdID,btTDID,true);
}

function btComputeBalance(tdID,btTDID,doUpdateTotal){
	balanceField = getFormElementByName("td["+tdID+"][btTD]["+btTDID+"][balance]");
	balanceField2 = getFormElementByName("td["+tdID+"][btTD]["+btTDID+"][balance2]");

	taxDueField = getFormElementByName("td["+tdID+"][btTD]["+btTDID+"][taxDue]");
//	taxDueField2 = getFormElementByName("td["+tdID+"][btTD]["+btTDID+"][taxDue2]");

	earlyPaymentDiscountField = getFormElementByName("td["+tdID+"][btTD]["+btTDID+"][earlyPaymentDiscount]");
	advancedPaymentDiscountField = getFormElementByName("td["+tdID+"][btTD]["+btTDID+"][advancedPaymentDiscount]");
	amenstyField = getFormElementByName("td["+tdID+"][btTD]["+btTDID+"][amnesty]");
	penaltyField = getFormElementByName("td["+tdID+"][btTD]["+btTDID+"][penalty]");
	totalPaidField = getFormElementByName("td["+tdID+"][btTD]["+btTDID+"][totalPaid]");

	balance = parseFloat(unformatCurrency(balanceField.value));
	taxDue = parseFloat(unformatCurrency(taxDueField.value));
	earlyPaymentDiscount = parseFloat(unformatCurrency(earlyPaymentDiscountField.value));
	advancedPaymentDiscount = parseFloat(unformatCurrency(advancedPaymentDiscountField.value));
	penalty = parseFloat(unformatCurrency(penaltyField.value));
	totalPaid = parseFloat(unformatCurrency(totalPaidField.value));

	balance = taxDue;
	if(advancedPaymentDiscount > 0){
		balance = taxDue - advancedPaymentDiscount;
	}
	else if(earlyPaymentDiscount > 0){
		balance = taxDue - earlyPaymentDiscount;
	}

	if(!amenstyField.checked){
		balance = balance + penalty;
		balance = balance - totalPaid;
	}
	else{
		balance = balance - totalPaid;
		if(balance<0){
			balance = 0;
		}		
	}

	balanceField.value = formatCurrency(balance.toString());
	balanceField2.value = balanceField.value;

	if(doUpdateTotal==true){
		updateTotals();
	}
//	taxDueField2.value = taxDueField.value;
}

function payAmount(){
	goPay = false;
	payAmountArray = new Array();
	balanceAmountArray = new Array();
	amnestyCheckboxValuesArray = new Array();

	j=0;
	for(i=0 ; i<document.RPTOPForm.elements.length ; i++){
		elementName = document.RPTOPForm.elements[i].name;

		if(elementName.indexOf("[amnesty]") >= 0){
			amnestyCheckboxValuesArray[j] = document.RPTOPForm.elements[i];
		}
		else if(elementName.indexOf("[balance2]") >= 0){
			balanceAmountArray[j] = document.RPTOPForm.elements[i];
		}
		else if(elementName.indexOf("[amountPaid]") >= 0){
			payAmountArray[j] = document.RPTOPForm.elements[i];
			j++;
		}
	}

	for(k=0; k < payAmountArray.length ; k++){
		if(payAmountArray[k].value==""){
			payAmountArray[k].value = formatCurrency(0);
		}
	}

	for(k=0; k < payAmountArray.length ; k++){
		if(!isNumeric(payAmountArray[k].value)){
			alert("Amount must be numeric.");
			payAmountArray[k].focus();
			return false;
		}
		else{
			if(parseFloat(unformatCurrency(balanceAmountArray[k].value)) < parseFloat(unformatCurrency(payAmountArray[k].value))){
				alert("Amount must not be greater than balance due.");
				payAmountArray[k].focus();
				goPay = false;
				return false;
			}
			else if(payAmountArray[k].value.indexOf("-") >= 0){
				alert("Amount must not be negative.");
				payAmountArray[k].focus();
				goPay = false;
				return false;				
			}
			else{
				if(payAmountArray[k].value > 0){
				
					if(parseFloat(unformatCurrency(balanceAmountArray[k].value)) > parseFloat(unformatCurrency(payAmountArray[k].value))){
						alert("Amount must not be less than balance due. All forms of partial payment are not allowed.");
						payAmountArray[k].focus();
						goPay = false;
						return false;			
					}
					else{				
						goPay = true;
					}
				}
			}
		}
	}

	if(goPay){
		document.RPTOPForm.formAction.value = "applyPayment";
		document.RPTOPForm.action = "PaymentReceiptDetails.php{Session}";
		document.RPTOPForm.submit();
	}
	else{
		alert("Cannot pay given amount.");
		return false;
	}

}



function MM_reloadPage(init) {  //reloads the window if Nav4 resized
  if (init==true) with (navigator) {if ((appName=="Netscape")&&(parseInt(appVersion)==4)) {
    document.MM_pgW=innerWidth; document.MM_pgH=innerHeight; onresize=MM_reloadPage; }}
  else if (innerWidth!=document.MM_pgW || innerHeight!=document.MM_pgH) location.reload();
}
MM_reloadPage(true);

function MM_showHideLayers() { //v6.0
  var i,p,v,obj,args=MM_showHideLayers.arguments;
  for (i=0; i<(args.length-2); i+=3) if ((obj=MM_findObj(args[i]))!=null) { v=args[i+2];
    if (obj.style) { obj=obj.style; v=(v=='show')?'visible':(v=='hide')?'hidden':v; }
    obj.visibility=v; }
}

